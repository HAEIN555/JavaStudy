package study;

public class MethodEX02_01 {
	public static void X() { // 전역변수를 지정...? 
		for(int a=0;a<5;a++) {
			System.out.print("*");
		}//void는 return이 생략// 결과형 return 값이 void일 경우 return문은 생략 가능하다. 
		return;
	}
	
	public static void main(String[] args) {
		for(int a=0;a<5;a++) {
			System.out.print("*");
		}
		System.out.println("\nHello");
		
		for(int a=0;a<5;a++) {
			System.out.print("*");
		}
		System.out.println("\nJAVA");
		
		for(int a=0;a<5;a++) {
			System.out.print("*");
		}
		System.out.println("\n!!!!!");
		
		for(int a=0;a<5;a++) {
			System.out.print("*");
		}
}	
}
