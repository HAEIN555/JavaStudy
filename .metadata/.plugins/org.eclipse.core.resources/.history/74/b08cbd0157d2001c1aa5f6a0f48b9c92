package study;
import java.io.IOException;
// 자바 IO (Input/Output)에 대한 예외를 처리하는 객체를 추가하는 코드 입니다.

public class D001 {

	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub
		// Ex01 - 아스키 코드 입력받기
		// 아스키 코드 값입력 받기---> 문자
		/* 0-9 : 48-57까지
		 * A-Z : 65-90까지
		 * a-z : 97-122까지
		 */
	//try {
		System.out.print("문자입력"); // 문자입력하세요
		int i = System.in.read(); // 시스템을 읽으세요 System.in.read()이거는 아스키 코드 값을 읽어라
		System.out.println("ASCII Code값 :" +i); 
		// 아스키 코드로 변환 그냥 enter치면 값이 13이 나온다.
	//} catch (IOE Exception e) { //무언가를 예외처리 해주는거...?
 // e. printStackTrace(); }
		
		
	}
}
