package study;

public class D005 {

	public static void main(String[] args) {
		/* while문을 이용해서  구구단을 출력하세요!
		 */
		int i=2,j;
		while(i<=9) {
			j=1;
			while(j<=9) {
				System.out.printf("%dX%d=%d\n",i,j,(i*j));
				j++;
				
			}System.out.println();
			i++;
			
		}
		
	// while문은 초기값이 바깥쪽에 있다 만약 2중 while문인경우 
	// 안쪽의 while 문도 반드시 초기화 해주어야 한다!
		// do while문으로 바꾸어 보장
		
		
	}

}
